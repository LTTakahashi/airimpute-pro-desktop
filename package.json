{
  "name": "airimpute-pro-desktop",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Professional desktop application for air quality data imputation using scientifically validated methods",
  "author": "AirImpute Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit -p tsconfig.build.json && vite build",
    "build:ci": "npm run build",
    "build:frontend": "vite build",
    "build:strict": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "node scripts/run-platform-script.cjs test",
    "test:windows": "cross-env NODE_ENV=test vitest --run",
    "test:default": "NODE_ENV=test vitest",
    "test:unit:windows": "cross-env NODE_ENV=test vitest --run --coverage",
    "test:integration:windows": "node scripts/run-integration-tests-windows.cjs",
    "test:integration:windows:direct": "cross-env NODE_ENV=test vitest --run --config vitest.integration.config.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:windows": "cross-env NODE_ENV=production npm run prebuild:windows && tauri build --target x86_64-pc-windows-msvc",
    "prebuild:windows": "powershell -ExecutionPolicy Bypass -File scripts/copy-python-dlls.ps1",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preinstall": "node scripts/check-windows-deps.cjs || echo 'Skipping Windows deps check'",
    "postinstall": "node scripts/unified-post-install.cjs",
    "postinstall:windows": "node scripts/windows-post-install.cjs || echo 'Skipping Windows post-install'",
    "postinstall:default": "node scripts/post-install.cjs || echo 'Skipping post-install'"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-spring/three": "^9.7.3",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^9.1.4",
    "@tanstack/react-query": "^5.17.9",
    "@tanstack/react-query-devtools": "^5.17.9",
    "@tauri-apps/api": "^1.5.3",
    "@types/katex": "^0.16.7",
    "@types/recharts": "^2.0.1",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "framer-motion": "^12.16.0",
    "katex": "^0.16.10",
    "lucide-react": "^0.309.0",
    "plotly.js": "^2.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^10.1.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.15.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.158.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.9",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/chroma-js": "^3.1.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.7",
    "@types/plotly.js": "^2.12.31",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-plotly.js": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^13.0.4",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "optionalDependencies": {
    "@microsoft/windows-sdk": "^10.0.0",
    "windows-build-tools": "^5.2.2",
    "@rollup/rollup-win32-x64-msvc": "^4.9.6",
    "@tauri-apps/cli-win32-x64-msvc": "^1.5.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
